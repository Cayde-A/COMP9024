long result;

// f(n) = 1 * 2 * ... * (n-1) * n
f(n) {
  if (SccLessEqual(n, 1)) { // n <= 1
    return 1;
  }
  else {
    return f(n-1) * n;
  }
}


main(argc, argv, env) {
  long i;
  
  i = 1;
  // f(i) = 1 * 2 * ... * i
  while (SccLessEqual(i, 5)) { // i <= 5
    result = f(i);
    output(result);
    i = i + 1;
  }
  return 0;
}


